{
  "name": "express-app",
  "version": "1.0.0",
  "description": "A simple expressjs app",
  "main": "gulpfile.js",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-firebase-middleware": "^2.0.0",
    "firebase-admin": "^5.11.0",
    "mongoose": "^5.7.5"
  },
  "devDependencies": {
    "mocha": "^5.0.4",
    "standard": "^11.0.1"
  },
  "scripts": {
    "start": "npm run symlink ; node app/server",
    "symlink": "cd node_modules && ln -nsf ../app && cd ../",
    "test": "mocha $(find app -name '*test.js')"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iamchandujs/express-app.git"
  },
  "keywords": [
    "express",
    "js",
    "app",
    "structure"
  ],
  "author": "Chandu J S",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamchandujs/express-app/issues"
  },
  "homepage": "https://github.com/iamchandujs/express-app#readme",
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  }
}
